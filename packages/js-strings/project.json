{
  "sourceRoot": "packages/js-strings/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/js-strings",
        "main": "packages/js-strings/src/index.ts",
        "tsConfig": "packages/js-strings/tsconfig.lib.json",
        "assets": ["packages/js-strings/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/js-strings/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/js-strings"],
      "options": {
        "jestConfig": "packages/js-strings/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
